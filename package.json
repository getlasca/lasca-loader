    "css-loader": "^6.0.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "jest": "^26.6.3",
    "lasca-loader": "../lasca-loader",
    "memfs": "^3.2.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3",
    "vue": "^2.6.14",
    "vue-loader": "^16.3.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.27.2"
  },
  "dependencies": {
    "jsdom": "^16.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
